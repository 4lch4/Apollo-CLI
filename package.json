    "prepublishOnly": "bun run build",
    "pretty": "prettier --write .",
    "start": "bun run dist/index.js",
    "test": "bun test"
  },
  "dependencies": {
    "@4lch4/backpack": "^1.3.6",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dayjs": "^1.11.10",
    "figlet": "^1.7.0",
    "inquirer": "^9.2.12",
    "nanoid": "^5.0.4",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "bun-types": "^1.0.15",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.7",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
